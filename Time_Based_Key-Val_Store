class TimeMap:

    def __init__(self):
        self.store = {} # key : list of [val, timestamp]
        

    def set(self, key: str, value: str, timestamp: int) -> None:
        if key not in self.store:
            self.store[key] = []
        self.store[key].append([value, timestamp])
        

    def get(self, key: str, timestamp: int) -> str:
        res = ""
        values = self.store.get(key, [])

        # binary search
        l, r = 0, len(values) - 1
        while l <= r:
            m = (l + r) // 2
            if values[m][1] <= timestamp: # less than or equal is completely acceptable
                res = values[m][0]
                l = m + 1
            else: # no res, as invalid value
                r = m - 1
        return res

        
